  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutility/live-react.git"
  },
  "bugs": {
    "url": "https://github.com/wutility/live-react.git/issues"
  },
  "homepage": "https://github.com/wutility/live-react.git#readme",
  "keywords": [
    "react",
    "live",
    "prism",
    "syntax highlighter",
    "component"
  ]
